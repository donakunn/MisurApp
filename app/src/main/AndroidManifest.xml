<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.misurapp">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <!-- Permissions required by GoogleAuthUtil -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.MANAGE_ACCOUNTS" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MisurAppInstrumentBaseActivity" />
        <activity android:name=".MisurAppBaseActivity" />
        <activity android:name=".BluetoothConnectionActivity" />
        <activity android:name=".ThermometerActivity" />
        <activity android:name=".StepActivity" />
        <activity android:name=".HygrometerActivity" />
        <activity android:name=".CompassActivity" />
        <activity android:name=".BarometerActivity" />
        <activity android:name=".AltimeterActivity" />
        <activity android:name=".ScoutMasterDatabaseActivity" />
        <activity android:name=".BoyscoutDBValuesActivity" />
        <activity android:name=".LightActivity" />
        <activity android:name=".ListaStrumentiActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>